<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Model;

class LoginOtp extends Model
{
    protected $fillable = [
        'user_id',
        'contact',
        'otp_code',
        'otp_type',
        'is_verified',
        'expires_at',
        'verified_at',
        'attempt_count',
        'max_attempts',
        'ip_address'
    ];

    protected $casts = [
        'is_verified' => 'boolean',
        'expires_at' => 'datetime',
        'verified_at' => 'datetime',
    ];
}

